									<a href="javascript:document.getElementById('searchbox').submit();">Search</a>

								</p>
							</form> -->
						</section>
						
						<!-- /Block search module TOP -->

						<!-- Block user information module HEADER -->
						<section ng-controller="GlobalCtrl">
							<section id="header_user" class="blockuserinfo-cart header-box" ng-class="{closeCart: cartHandler == true}">
								<div id="shopping_cart" ng-controller="GlobalCtrl">
									<a href="javascript:void(0)"  rel="nofollow" ng-click="isCartOpen()">
										<strong class="opancart"></strong>
										<span class="shopping_cart_title">Cart</span>
										<span class="ajax_cart_quantity" >{{ReturnItems()}}</span>
									</a>
								</div>
							</section>
						</section>
						<section class="blockuserinfo header-box hidden-desktop hidden-tablet">
							<a ng-if="!User.info.CustomerEmail" href="/login" title="Login" class="login" rel="tooltip" data-placement="bottom" data-original-title="first tooltip">
								<span>
									<i class="icon-lock"></i>
								</span>
							</a>
							<a ng-if="User.info.CustomerEmail" href="javascript:void(0)" ng-click="Logout()" title="Logout" class="login" rel="tooltip" data-placement="bottom" data-original-title="first tooltip">
								<span>
									<i class="icon-unlock"></i>
								</span>
							</a>
						</section>

						<!-- Block permanent links module HEADER -->
						<section class="header-box blockpermanentlinks-header">
							<ul id="header_links">
								<li><a href="/" class="header_links_home">Home</a>
								</li>
								<li id="header_link_contact"><a href="javascript:void(0)" title="contact">Contact</a>
								</li>
								<li id="header_link_sitemap"><a href="/page/about" title="sitemap">About Us</a>
								</li>
							</ul>

							<div class="mobile-link-top header-button" ng-controller="GlobalCtrl">
								<h4 ng-click="openMenu">
									<span class="title-hed" ng-click="openMenu()"></span>
									<span class="arrow_header_top_menu arrow_header_top" ng-click="openMenu()"></span>
								</h4>
								<ul id="mobilelink" class="list_header" ng-class="{show: enableDropdown == true}" ng-repeat="Navigation in ListNavigation | filter : 'main-menu'">
									
									<li ng-repeat="Nav in Navigation.link_list"  ng-class="{sfHoverForce:  $location.path().indexOf('{{Nav.commaSeprated}}') >= 0}" ng-show="Nav.commaSeprated">
									<a href="{{Nav.commaSeprated}}">{{Nav.name}}</a>
									</li>
									
									<!-- <li><a href="/" class="header_links_home">Home</a>
									</li>
									<li id="header_link_contact"><a href="javascript:void(0)"  title="contact">Contact</a>
									</li>
									<li id="header_link_sitemap"><a href="/page/about" title="sitemap">About Us</a>
									</li> -->
								</ul>
							</div>
						</section>
						<!-- /Block permanent links module HEADER -->


						<div class="clearfix"></div>
						<div id="menu-wrap" class="clearfix">
							<div id="menu-trigger">Categories
								<span class="menu-icon"></span>
							</div>
							<ul id="menu-custom" ng-repeat="Navigation in ListNavigation | filter : 'main-menu'">
								<li ng-repeat="Nav in Navigation.link_list"  ng-class="{sfHoverForce:  $location.path().indexOf('{{Nav.commaSeprated}}') >= 0}" ng-show="Nav.commaSeprated">
									<a href="{{Nav.commaSeprated}}">{{Nav.name}}</a>
								</li>
							</ul>
						</div>
						<!-- Minic Slider -->
						<div ng-if="$location.path() == '/' && ThemeSettings.theme_settings.slide_1_img">
							<div id="minic_slider" class="theme-default"  class="hidden-phone">
								<div id="slider" class="nivoSlider">
									<img ng-if="ThemeSettings.theme_settings.slide_1_img && ThemeSettings.theme_settings.slide_1_show == true" src="{{ThemeSettings.theme_settings.slide_1_img}}" class="slider_image">
									<img ng-if="ThemeSettings.theme_settings.slide_2_img && ThemeSettings.theme_settings.slide_2_show == true" src="{{ThemeSettings.theme_settings.slide_2_img}}" class="slider_image">
									<img ng-if="ThemeSettings.theme_settings.slide_3_img && ThemeSettings.theme_settings.slide_3_show == true" src="{{ThemeSettings.theme_settings.slide_3_img}}" class="slider_image">
									<img ng-if="ThemeSettings.theme_settings.slide_4_img && ThemeSettings.theme_settings.slide_4_show == true" src="{{ThemeSettings.theme_settings.slide_4_img}}" class="slider_image">
								</div>
							</div>
							<div ng-if="ThemeSettings.theme_settings.slide_1_img">
								<script type="text/javascript">
									$(function(){
							            $('#slider').nivoSlider();
									});
								</script>
					        </div>

							<div id="customcontent_top2"  class="hidden-phone" ng-if="ThemeSettings.theme_settings.slide_1_img">
								<ul class="customcontent-home">
									<li class="num-1">
										<a href="#">
											<img src="http://livedemo00.template-help.com/prestashop_44889/modules/customcontent2/images/d585c113aff322a37ba072c463fbeef6.jpg" alt="">
										</a>
									</li>
								</ul>
								<div class="clearfix"></div>
							</div>
							<div class="clearfix"></div>

							<div id="customcontent_top">
								<ul class="customcontent-top">
									<li ng-repeat="products in ListProduct | CustomFilter:{collection:'featured-products'}" class="num-{{$index+1}}">
										<a href="/product/{{products.productName}}">
											<img src="{{products.productImage[0].Image}}" alt="{{products.productName}}">
										</a>
										<a class="abs" href="/product/{{products.productName}}">
											<div class="title">{{products.productName.slice(0,20)}}<span ng-if="products.productName.length > 20">....</span></div>
											<p style="" ng-bind-html="products.productDescription.slice(0,65) + '...'">
											<div class="title3">{{SettingGeneral.settings.currencies_format}}&nbsp;{{products.productPrice}}<i class="icon-circle-arrow-right"></i>
											</div>
										</a>
									</li>
									<div class="clearfix"></div>
								</ul>
								<div class="clearfix"></div>
							</div>

						</div>
					</div>
				</header>

