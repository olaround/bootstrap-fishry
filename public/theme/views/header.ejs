<div id="Loader" style="position:fixed;left:0;top:0;padding:5px;">loading...</div>
	<div id="wrapp">
		<div id="wrapp_2">
			<div class="header_bg">

				<header id="header" class="container ">
					<div id="header-inner" ng-controller="GlobalCtrl">
						<a id="header_logo" href="/" title="Drug Store">
							<!-- <img class="logo" src="/img/theme/logo.png" alt="Drug Store" width="200" /> -->
							<img ng-if="ThemeSettings.theme_settings.logo_img" class="logo" src="{{ThemeSettings.theme_settings.logo_img}}" alt="Drug Store" width="200" />
						</a>

						<!-- MODULE Block cart -->
						<section id="cart_block" class="block column_box exclusive" style="display:block" ng-if="cartHandler">
							<h4>
								<span>Cart</span>
								<span class="column_icon_toggle"></span>
							</h4>
							<div class="block_content toggle_content">

								<!-- block summary -->
								<div id="cart_block_summary" class="collapsed">
									<span class="ajax_cart_quantity" style="">{{ReturnItems()}}</span>
									<span class="ajax_cart_product_txt_s" ng-if="ReturnItems() > 1">Products</span>
									<span class="ajax_cart_product_txt" ng-if="ReturnItems() <= 1">Product</span>
									<span class="ajax_cart_total" style="">{{SettingGeneral.settings.currencies_format}}&nbsp;{{ReturnTotal()}}</span>
									<span class="ajax_cart_no_product" ng-hide="IfCart()">(empty)</span>
								</div>

								<div id="cart_block_list" class="expanded" style="display:block">
									<dl class="products clearfix">
										<dt id="cart_block_product_1_0_0" class="clearfix first_item" ng-repeat="Items in Cart">
											<a class="cart-images" href="/product/{{Items.productInfo.productName}}">
												<img src="{{Items.productInfo.productImage[0].Image}}" alt="" title="{{Items.productInfo.productName}}">
											</a>
											<span class="quantity-formated">
												<span class="quantity" style="opacity: 1;">{{Items.quantity}}</span>x
											</span>
											<a class="cart_block_product_name product_link" href="/product/{{Items.productInfo.productName}}" title="A180 Injectable Antibiotic">{{Items.productInfo.productName.slice(0,10)}}<span ng-if="products.productName.length > 10">....</span></a>
											
											<span class="remove_link">
												<a class="ajax_cart_block_remove_link" rel="nofollow" href="javascript:void(0)" ng-click="RemoveCartItem(Items.productID)"></a>
											</span>
											<span class="price">{{SettingGeneral.settings.currencies_format}}&nbsp;{{Items.productInfo.productPrice * Items.quantity}}</span>
										</dt>
									</dl>
									<div class="cart-prices" ng-if="ReturnItems() > 0 ">
										<div class="cart-prices-block">
											<span id="cart_block_total" class="price
											 ajax_block_cart_total" ng-if="ReturnItems()">{{SettingGeneral.settings.currencies_format}}&nbsp;{{ReturnTotal()}}</span>
											<span>Total</span>
										</div>
									</div>
									<p id="cart-buttons" class="" ng-if="ReturnItems() > 0">
										<!-- <a href="/cart" class="button_mini" title="View my shopping cart" rel="nofollow">Cart</a> -->
										<a href="/cart" id="button_order_cart" class="exclusive" title="Checkout" rel="nofollow">cart</a>
									</p>
								</div>
								<p class="no_products" id="cart_block_no_products" ng-if="ReturnItems() <=0 ">No products</p>
								<!-- <h1>{{ifCart()}}</h1> -->
							</div>
						</section>
						<!-- /MODULE Block cart -->

						<!-- block seach mobile -->
						<!-- Block search module TOP -->
						<section id="search_block_top" class="header-box">
							
							<section class="customerInfo hidden-phone">
								<div ng-if="User.info.CustomerEmail"> Welcome <strong class="customerName">{{User.info.CustomerFirstName}}</strong></div>
								<div ng-if="User.info.CustomerEmail"> <a class="btn btn-danger" href="javascript:void(0)" ng-click="Logout()"> Logout </a></div>

								<div ng-if="!User.info.CustomerEmail">
									<!-- <a class="btn btn-info loginBtn" href="./login">Login</a> -->
									<a class="btn btn-info loginBtn" href="./login">
										SIGN IN </a> Don't have an account? <a class="btn btn-info loginBtn createNewBtn" href="./signup">CREATE ONE</a>
								</div>
								<!-- <div ng-if="!User.info.CustomerEmail"> <a class="btn btn-info" href="./signup"> Sign Up </a></div> -->
							
							</section>
							
							<!-- <form method="get" action="#" id="searchbox">
								<p>
									<label for="search_query_top">Search</label>
									<input type="hidden" name="controller" value="search" />
