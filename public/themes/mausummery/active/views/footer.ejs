<div id="page">
<!-- FOOTER -->
		<footer>
			
			<!-- CONTAINER -->
			<div class="container" data-animated='fadeInUp'>
				
				<!-- ROW -->
				<div class="row">
					
					<div class="col-lg-2 col-md-2 col-sm-3 col-xs-6 col-ss-12 padbot30" ng-if="SettingGeneral">
						<h4>Contacts</h4>
						<div class="foot_address"><span ng-if="SettingGeneral.name">{{SettingGeneral.name}}</span><span ng-if="SettingGeneral.address1" style="display: inline-block;letter-spacing: 1px;">{{SettingGeneral.address1}},</span><span ng-if="SettingGeneral.city" style="display: inline-block;letter-spacing: 1px;">{{SettingGeneral.city}},</span><span ng-if="SettingGeneral.country" style="display: inline-block;letter-spacing: 1px;">{{SettingGeneral.country}}</span></div>
						<div class="foot_phone" ng-if="SettingGeneral.phone"><a>{{SettingGeneral.phone}}</a></div>
						<div class="foot_mail" ng-if="SettingGeneral.contactEmail"><a href="" >{{SettingGeneral.contactEmail}}</a></div>
						
					</div>
					
					<div class="col-lg-2 col-md-2 col-sm-3 col-xs-6 col-ss-12 padbot30">
						<h4>Information</h4>
						<ul class="foot_menu" ng-repeat="Navigation in ListNavigation | filter : 'footer'">
							<li class="sub-menu first" ng-repeat="Nav in Navigation.link_list"  ng-class="{active:  $location.path() == '/'}" ng-show="Nav.optionSelected == 'frontpage'">
                                <a href="/">
                                    {{Nav.name}}
                                </a>
                             </li>
							<li class="sub-menu first" ng-repeat="Nav in Navigation.link_list"  ng-class="{active:  $location.path().indexOf('{{Nav.commaSeprated}}') >= 0}" ng-show="Nav.commaSeprated">
                                <a href="{{Nav.commaSeprated}}">
                                   {{Nav.name}}
                                </a>
                             </li>
                            <li class="sub-menu first" ng-repeat="Nav in Navigation.link_list"  ng-class="{active:  $location.path().indexOf('{{Nav.http}}') >= 0}" ng-show="Nav.http">
                                <a href="{{Nav.http}}" target="_blank">
                                    {{Nav.name}}
                                </a>
                            </li>
							
						</ul>
					</div>
					
					<div class="respond_clear_480"></div>
					
					<div class="col-lg-4 col-md-4 col-sm-6 padbot30">
