							<a ng-if="ThemeSettings.theme_settings.facebook_link" href="{{ThemeSettings.theme_settings.facebook_link}}" target="_blank" ><i class="fa fa-facebook"></i></a>
							<a ng-if="ThemeSettings.theme_settings.google_link" href="{{ThemeSettings.theme_settings.google_link}}" target="_blank" ><i class="fa fa-google-plus"></i></a>
