						<!-- MODULE Block cart -->
						<section id="cart_block" class="block column_box exclusive" style="display:block" ng-if="cartHandler">
							<h4>
								<span>Cart</span>
								<span class="column_icon_toggle"></span>
							</h4>
							<div class="block_content toggle_content">

								<!-- block summary -->
								<div id="cart_block_summary" class="collapsed">
									<span class="ajax_cart_quantity">{{ReturnItems()}}</span>
									<span class="ajax_cart_product_txt_s" ng-if="ReturnItems() > 1">Products</span>
									<span class="ajax_cart_product_txt" ng-if="ReturnItems() <= 1">Product</span>
									<span class="ajax_cart_total">{{SettingGeneral.settings.currencies_format}}&nbsp;{{ReturnTotal()}}</span>
									<span class="ajax_cart_no_product" ng-hide="IfCart()">(empty)</span>
								</div>

								<div id="cart_block_list" class="expanded" style="display:block">
									<dl class="products clearfix" ng-repeat="(keyMain,Items) in Cart track by $index">
										<dt id="cart_block_product_1_0_0" class="clearfix first_item" ng-show="Items.productID">
											<a class="cart-images" href="/product/{{Items.productInfo.productUrl}}">
												<img ng-if="Items.productInfo.productImage[0].Image" src="{{Items.productInfo.productImage[0].Image}}" alt="" title="{{Items.productInfo.productName}}">
												<img ng-if="Items.productInfo.productImage[0].Image == undefined" ng-src="{{BaseUrl}}img/theme/default.jpg" alt="{{Items.productInfo.productName}}">
											</a>
											<span class="quantity-formated">
												<span class="quantity" style="opacity: 1;">{{Items.quantity}}</span>x
											</span>
											<a class="cart_block_product_name product_link" href="/product/{{Items.productInfo.productUrl}}" title="Items.productInfo.productName">{{Items.productInfo.productName.slice(0,10)}}<span ng-if="products.productName.length > 10">....</span></a>
											
											<span class="remove_link">
												<a class="ajax_cart_block_remove_link" rel="nofollow" href="javascript:void(0)" ng-click="RemoveCartItem(Items.productID,false)"></a>
											</span>
											<span class="price">
											{{Items.productInfo.productPrice * Items.quantity | nfcurrency:SettingGeneral.settings.currencies_format+' '}}
											</span>
										</dt>

										<dt ng-hide="Items.productID" id="cart_block_product_1_0_0" class="clearfix first_item" ng-repeat="(key,itm) in Items track by $index">
											<a class="cart-images" href="/product/{{itm.productInfo.productUrl}}">
												<img ng-if="itm.productInfo.productImage[0].Image" src="{{itm.productInfo.productImage[0].Image}}" alt="" title="{{itm.productInfo.productName}}">
												<img ng-if="itm.productInfo.productImage[0].Image == undefined" ng-src="{{BaseUrl}}img/theme/default.jpg" alt="{{itm.productInfo.productName}}">
											</a>
											<span class="quantity-formated">
												<span class="quantity" style="opacity: 1;">{{itm.quantity}}</span>x
											</span>
											<a class="cart_block_product_name product_link" href="/product/{{itm.productInfo.productUrl}}" title="itm.productInfo.productName">{{itm.productInfo.productName.slice(0,10)}}<span ng-if="products.productName.length > 10">....</span> <span ng-if="key" class="attributesName">- ( {{key}} )</span></a>
											
											<span class="remove_link">
												<a class="ajax_cart_block_remove_link" rel="nofollow" href="javascript:void(0)" ng-click="RemoveCartItem(itm.productID,key)"></a>
											</span>
											<span class="price">
											{{itm.productInfo.newPrice * Itm.quantity | nfcurrency:SettingGeneral.settings.currencies_format+' '}}	
											</span>
										</dt>

									</dl>
									<div class="cart-prices" ng-if="ReturnItems() > 0 ">
										<div class="cart-prices-block">
											<span id="cart_block_total" class="price
											 ajax_block_cart_total" ng-if="ReturnItems()">{{ReturnTotal() | nfcurrency:SettingGeneral.settings.currencies_format+' '}}</span>
											<span>Total</span>
										</div>
									</div>
									<p id="cart-buttons" class="" ng-if="ReturnItems() > 0">
										<!-- <a href="/cart" class="button_mini" title="View my shopping cart" rel="nofollow">Cart</a> -->
										<a href="/cart" id="button_order_cart" class="exclusive" title="Checkout" rel="nofollow">cart</a>
									</p>
								</div>
