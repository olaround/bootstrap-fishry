<html ng-app="aeCommerce" ng-controller="AECtrl">
  <head>
   <meta name="fragment" content="!">
    <base href="/">
	 <title ng-bind="title">
                 <%= title %>
              </title>
		<% if (typeof(meta) != "undefined") { %>
			  <% for (var metaItem in meta) { %>
			  <% if (meta[metaItem] && metaItem) { %>
				<meta name="<%= metaItem %>" content="<%= meta[metaItem] %>" />
				<% } %>
				<% } %>
          <% } %>
		  <% if (typeof(og) != "undefined") { %>
			  <% for (var ogItem in og) { %>
			   <% if (og[ogItem] && ogItem) { %>
				<meta property="<%= ogItem %>" content="<%= og[ogItem] %>" />
				<% } %>
				<% } %>
          <% } %>
             
			   <%- requireServerVars() %>
              </head>
              <body id="index">
			  <div id='loadingbar'></div>
			  <!--<div id="Loader" style="position:fixed;left:0;top:0;padding:5px;">loading...</div>-->
				<div class="container" style="display:none;">
               	 <div class="col-md-12 column fixed">
                                <div class="col-md-12 bg-white">
                                <div align="center">
                                    <button class="btn btn-default btn-info" ng-click="isCollapsed = !isCollapsed">
												   <em class="glyphicon glyphicon-shopping-cart"></em>
                                                    <span ng-if="IfCart()">&nbsp;-  ITEMS ({{ReturnItems()}}) </span>
                                                    <span ng-if="IfCart()">&nbsp;-  Price (Rs.{{ReturnTotal()}})</span>
                                                        <br>
                                    </button>
                                    <div collapse="isCollapsed" class="max-width">
                                      
										
										<table ng-show="IfCart()" class="table">
										  <thead>
											<tr>
											  <th class="col-md-4">
												Product
											  </th>
											  <th class="col-md-4">
												Price
											  </th>
											  <th class="col-md-4">
												Quantity
											  </th>
											  <th class="col-md-4 min200">
												Total Price
											  </th>
											  <th class="col-md-4">
												Action
											  </th>
											</tr>
										  </thead>
										  
										  <tbody ng-repeat="(keyMain,Itemss) in Cart track by $index">
											<tr ng-show="Itemss.productID">
											  <td class="col-md-4">
												{{Itemss.productInfo.productName}} 
											  </td>
											  <td class="col-md-4">
												{{SettingGeneral.settings.currencies_format}}. {{Itemss.price}}
											  </td>
											  <td class="col-md-4">
												<input style="width:62px;" ng-change="changeQuantity()"  min='1'  ng-model="Itemss.quantity" type="number" >
											  </td>
											  <td class="col-md-4">
												{{SettingGeneral.settings.currencies_format}}. {{Itemss.price * Itemss.quantity}}
											  </td>
											  <td>
												<span class="glyphicon glyphicon-remove" ng-click="RemoveCartItem(Itemss.productID,false)">
												</span>
											  </td>
											</tr>
											<tr ng-hide="Itemss.productID" ng-repeat="(key,itm) in Itemss track by $index">
											  <td class="col-md-4">
												{{itm.productInfo.productName}} - ( {{key}} )
											  </td>
											  <td class="col-md-4">
												{{SettingGeneral.settings.currencies_format}}. {{itm.price}}
											  </td>
											  <td class="col-md-4">
												<input style="width:62px;" ng-change="changeQuantity()"  min='1'  ng-model="itm.quantity" type="number" >
											  </td>
											  <td class="col-md-4">
												{{SettingGeneral.settings.currencies_format}}. {{itm.price * itm.quantity}}
											  </td>
											  <td>
												{{}}
												<span class="glyphicon glyphicon-remove" ng-click="RemoveCartItem(itm.productID,key)">
												</span>
											  </td>
											</tr>
										  </tbody>
										</table>
  										  <div ng-show="IfCart()">
											  <div class="clearfix">
											  </div>
											  <div class="form-group">
												<a class="btn btn-info pull-left" ng-click="isCollapsed = true" href="./cart">Checkout</a>
											  </div>
											  <hr />
											   <div class="col-md-12"  align="right">
												<h2  align="right" ng-if="ReturnTax()">
												  Total : {{ReturnTotal()- ReturnTaxPrice()| currency:SettingGeneral.settings.currencies_format+' '}}
												</h2>
											  </div>
											   <hr /> 
											  <div class="col-md-12"  align="right"  ng-if="ReturnTax()">
												<h2  align="right">
												  Tax : {{ReturnTaxPrice()| currency:SettingGeneral.settings.currencies_format+' '}} 
												</h2>
												<span>
												  Tax are dependent on shipping country
												</span>
												<hr />
											  </div>
											  <div class="clearfix">
											  </div>
											  
											  
											  <div class="col-md-12"  align="right">
												<h2>
												  Total : {{ReturnTotal()| currency:SettingGeneral.settings.currencies_format+' '}}
												</h2>
												 <hr />
											  </div>
											  <div class="clearfix">
											  </div>											  
											  
											</div>
                                        <h6 ng-hide="IfCart()" align="center">No Item Found</h6>
                                    </div>
                                </div>
                          </div>
                            </div>
				  <div class="row clearfix">
                          <div class="page-header">
                                 <div class="pull-left">
                                     <h1> <img width="100" alt="Logo" src="{{ThemeSettings.theme_settings.logo_img}}"> <small>{{SettingGeneral.name}}</small></h1>
                                </div>
                                 <div class="pull-right">
                                <span>Customer Info  :</span>
                                                    <span ng-if="User.info.CustomerEmail"> Welcome {{User.info.CustomerFirstName}}</span>
                                                    <span ng-if="User.info.CustomerEmail"> <a class="btn btn-danger" href="javascript:void(0)" ng-click="Logout()"> Logout </a></span>
													<span ng-if="User.info.CustomerEmail && !User.info.GuestAuth"> <a class="btn btn-danger" href="/reset_password"> Reset password </a></span>
													<span ng-if="User.info.CustomerEmail && !User.info.GuestAuth"> <a class="btn btn-danger" href="/account_info"> Update Account Info </a></span>
													<span ng-if="User.info.CustomerEmail && !User.info.GuestAuth"> <a class="btn btn-danger" href="/orders">My orders </a></span>
                                                    <span ng-if="!User.info.CustomerEmail"> <a class="btn btn-info" href="./login"> Login </a></span>
                                                    <span ng-if="!User.info.CustomerEmail  && ListPaymentGateways.accountsDisabled == 'optional'"> <a class="btn btn-info" href="./signup"> Sign Up </a></span>
        
                            </div>
                                <div class="clearfix"></div>
                           </div>
                           
                           <div class="row clearfix">
                            <div class="col-md-12 column">
                              <nav class="navbar navbar-default" role="navigation">
                                <div class="navbar-header">
                                  <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                    
                                    <span class="sr-only">
                                      Toggle navigation
                                    </span>
                                    <span class="icon-bar">
                                    </span>
                                    <span class="icon-bar">
                                    </span>
                                    <span class="icon-bar">
                                    </span>
                                  </button>
                                  <a class="navbar-brand" href="./"><em class="glyphicon glyphicon-home"></em></a>
                                </div>
                                
                                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
								
                                  <ul class="nav navbar-nav" ng-repeat="Navigation in ListNavigation | filter : 'main-menu'">
                                     <li ng-repeat="Nav in Navigation.link_list"  ng-class="{active:  $location.path() == '/'}" ng-show="Nav.optionSelected == 'frontpage'">
                                      <a href="/"  >
                                        {{Nav.name}}
                                      </a>
                                    </li>
									<li ng-repeat="Nav in Navigation.link_list"  ng-class="{active:  $location.path().indexOf('{{Nav.commaSeprated}}') >= 0}" ng-show="Nav.commaSeprated">
                                      <a href="{{Nav.commaSeprated}}"  >
                                        {{Nav.name}}
                                      </a>
                                    </li>
                                    <li ng-repeat="Nav in Navigation.link_list"  ng-class="{active:  $location.path().indexOf('{{Nav.http}}') >= 0}" ng-show="Nav.http">
                                      <a href="{{Nav.http}}" target="_blank"  >
                                        {{Nav.name}}
                                      </a>
                                    </li>
                                  </ul>
                                </div>
                              </nav>
                              <ul class="col-lg-12 breadcrumb" ng-controller="GlobalCtrl">
                                <li>
                                  <a href="#">
                                    Home
                                  </a>
                                </li>
                                <li ng-repeat="links in locationParam" ng-show="links">
                                  <a href="./{{locationParam[0]}}" ng-if="$index==0">
                                  {{links}}
                                  </a>
                                  <a href="./{{locationParam[0]}}/{{locationParam[1]}}" ng-if="$index==1">
                                  {{links}}
                                  </a>
                                  <a href="./{{locationParam[0]}}/{{locationParam[1]}}/{locationParam[2]}}" ng-if="$index==2">
                                  {{links}}
                                  </a>
                                  <a href="./{{locationParam[0]}}/{{locationParam[1]}}/{locationParam[2]}}/{locationParam[3]}}" ng-if="$index==3">
                                  {{links}}
                                  </a>
                                </li>
                              </ul>
                             <!-- <form class="navbar-form navbar-left" role="search"   ng-hide="locationParam[0] == 'search'">
                                    <div class="form-group">
											<input placeholder="Search By Title..." ng-model="SearchTitle" type="text" class="form-control">
											<input placeholder="Search By Collection..." ng-model="SearchCollection" type="text" class="form-control">
											<input placeholder="Search By Vendor..." ng-model="SearchVendor" type="text" class="form-control">
											<input placeholder="Search By Type..." ng-model="SearchType" type="text" class="form-control">
											<input placeholder="Search By Tags..." ng-model="SearchTags" type="text" class="form-control">
											
                                    </div>
                                    <button ng-click="SearchTitle = '';SearchCollection = '';SearchVendor = '';SearchType = '';SearchTags = '';" class="btn btn-default">
                                      Reset All
                                    </button>
                                  </form>-->
                            </div>
                          </div>
                           
                   </div>
                  
                  
				  
				 
				  